h1 New Blog Post 

form(name="input", action="newblog", method="post")
	p Title
		input(type="text", name="title", value="#{title}")

	p Content
		textarea(name='content', rows="20", cols="60")
			#{content}

	p Posted On
		input(type="date", name="postedOn", value="#{postedOn}")

	p Posted At
		input(type="time", name="postedAt", value="#{postedOn}")

	input(type="submit", value="Submit")

	a(href='/blogs') Cancel 
